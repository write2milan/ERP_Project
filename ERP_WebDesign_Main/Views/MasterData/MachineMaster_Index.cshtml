@model IEnumerable<ERP_WebDesign_Main.Models.MasterData_Model.MachineMaster_Model>

@{
    ViewBag.Title = "Machine Master";
}

<h2>Listing of Machines</h2>

<p>
    @Html.ActionLink("Create New", "MachineMaster_Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MachineName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MachineDesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InstallDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MCLife)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WarrantyPeriod)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OilRequired)
