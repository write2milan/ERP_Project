        <th>
            @Html.DisplayNameFor(model => model.PowerRequired)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StandardOutputQTY)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StandardOutputUnit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StandardRunningTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModifiedBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModifiedDateTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MachineName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MachineDesc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InstallDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MCLife)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WarrantyPeriod)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OilRequired)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PowerRequired)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StandardOutputQTY)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StandardOutputUnit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StandardRunningTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedDateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModifiedBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModifiedDateTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "MachineMaster_Edit", new { ItemId = item.MachineID }) |
            @Html.ActionLink("Details", "MachineMaster_Details", new { ItemId = item.MachineID }) @*|
            @Html.ActionLink("Delete", "Delete", new { id=item.MachineID })*@
        </td>
    </tr>
}

</table>
